    function pageLoad(sender, args) {
      $('#popoverlink').popover({ content: $("#ctl00_Main_panelInfoPopup")[0].innerHTML, html: true, show: true, trigger: 'click', placement: 'right' });
    }

    var _emailValue;
    var _emailChecked;

    function CheckMailLoginDone(response) {
      var status = response.d[0];

      if (status == "1") {
        jQuery("#email3").html(_emailValue);
        $('#emailExistsLogin').bs_modal({ backdrop: 'static' });
      }
      else {
        _emailChecked = "true";
        document.getElementById('ctl00_Main_btLogin').click();
      }
    }

  </script>

  

  
