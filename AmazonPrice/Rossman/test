  $(document).ready(function () {
    ROLSHOP.helpers.executeOnDocReadyAndAJAX(function () {
      $('#ctl00_Main_ShopCartPartsNormal_ShopCartParts_ctl00_ShopCartPositions_ctl00_ctl02_popoverBtn').popover({
        animation: 'True',
        container: 'body',
        content: <span class="text-bold">Aptamil Pronutra™ Folgemilch 2</span><p class="verkehrsbez">Folgemilch</p><p class="text-muted">800 g<br> (1 kg = 18,69 €)</p>mit patentierten Probiotics (GOS/FOS), LCP-Milupan®, mit Omega 3-Fettsäure ALA, altersgerecht sättigend, ab dem 6. Monat<p><p><a href="/produkt/359207/aptamil-pronutra-folgemilch-2.aspx">Lebensmittelinformationen (LMIV)<a></p></p><a href="/produkt/359207/aptamil-pronutra-folgemilch-2.aspx#warnhinweise">Anwendungs- und Warnhinweise</a>,
        delay: '{"show": 300, "hide": 600 }',
        html: 'True',
        placement: 'left',
        title: '',
        trigger: 'manual'
      });
      
      $('#ctl00_Main_ShopCartPartsNormal_ShopCartParts_ctl00_ShopCartPositions_ctl00_ctl02_popoverBtn').hover(function () {
        $(this).popover('show');
        var popOver = $('#' + $(this).attr('aria-describedby'));
        var timer;

        $(this).mouseout(function () {
          timer = setTimeout(function () {
              popOver.fadeOut(600)
            }, 200);
        })
        
        popOver.mouseover(
          function () {
            clearTimeout(timer);
            $(this).stop().fadeTo("fast", 1);
          }).mouseout(
          function () {
            $(this).stop().fadeOut(600);
          });
      });
      
    });    
  });
</script>

                                  
