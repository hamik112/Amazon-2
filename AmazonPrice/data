<script>
  P.when("jQuery").execute(function(jQuery) {
      P.when("reviewsLightbox-js").execute(function() {
      
       
       var reviewImageGallery = new ReviewGallery("R23ZCN16G99DOG_gallerySection_1C3AF1F0-3874-11E6-8144-359CF04D9C03", "DESKTOP");
       reviewImageGallery.initializeForReview("R23ZCN16G99DOG");
                        
        var widgetDom = jQuery("#R23ZCN16G99DOG_imageSection_1C3AF1F0-3874-11E6-8144-359CF04D9C03");
        var thumbnails = widgetDom.find(".review-image-thumbnail"); 
            thumbnails.each( function(index) {
                jQuery(this).bind("click", function() {
                    var popover = widgetDom.find(".a-popover-trigger");
                    popover.click();
                    P.when("reviewsLightbox-ready-R23ZCN16G99DOG_gallerySection_1C3AF1F0-3874-11E6-8144-359CF04D9C03").execute(function() {
                       reviewImageGallery.render();
                       reviewImageGallery.showImageAtIndex(index);
                    });
                });
            });
      });
  });
</script>












<a class="a-link-normal comment-link commentStripe" title="0" href="https://www.amazon.co.jp/review/R23ZCN16G99DOG/ref=cm_cr_dp_cmt?ie=UTF8&ASIN=B00IKEU3QE&channel=detail-glance&nodeID=3828871&store=home#wasThisHelpful">コメント</a>
