  if(undefined !== window.$Nav) {
    $Nav.when("$", "nav.inline").run("JPPoints", function($){
      var supraId = "nav-supra";
      var $supraXShop = $("#" + supraId);
      if ($supraXShop.length === 0) {
        var wrapperId = "nav-xshop-container";
