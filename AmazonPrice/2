    <div id="moreBuyingChoices_feature_div" class="feature" data-feature-name="moreBuyingChoices">




    

    
    
    
        
            
                
	                
	                
	                	















    
    
        
    
























<!-- First fetch if the fmaFallback is true -->

<!-- If fma fall back is true, lets filter the offers with the buyboxMerchantId -->
















    
    
    
      <div class="a-section a-spacing-medium">
        <div id="mbc-action-panel-wrapper" style="">
            
            
            
            
            
                <div id="mbc" data-asin="B00BSNACII" data-brand="Aptamil" data-product-group="grocery_display_on_website" data-timeout="Leider ist ein Problem aufgetreten." class="a-box-group">
                
                    
                    
