  $(document).delegate(\"#ddmZipCodeInput1, #ddmZipCodeInput2\", \"input\", function() {\n                if(isValidZipCode(retrieveZipCode())) {\n                    $(\"#ddmZipCodeUpdateBtn\").removeClass(\"a-button-disabled\");\n                } else {\n                    $(\"#ddmZipCodeUpdateBtn\").addClass(\"a-button-disabled\");\n                }\n            });\n            $(document).delegate(\"#ddmZipCodeUpdateBtn\", \"click\", function(event){\n                var zipCode = retrieveZipCode();\n                if ($(\"#ddmCustomerAddressCheckbox\").attr(\"checked\")) {\n                    updateDeliveryMessage(1,defaultShippingCountryCode,defaultShippingZipCode,1);\n                } else if(isValidZipCode(zipCode)) {\n                    $(\"#ddmZipCodeInput1\").removeClass(\"a-form-error\");\n                    $(\"#ddmZipCodeInput2\").removeClass(\"a-form-error\");\n                    $(\"#ddmInvalidZipInputAlert\").addClass(\"a-hidden\");\n                    updateDeliveryMessage(1,country,zipCode,0);\n                }\n            });\n            $(document).delegate(\"#ddmCustomerAddressCheckbox\", \"change\", function(){\n                if ($(this).attr(\"checked\")) {\n                    // disable iput boxes when customer default shipping address checkbox selected\n                    $(\"#ddmZipCodeInput1\").attr(\"disabled\", \"disabled\");\n                    $(\"#ddmZipCodeInput2\").attr(\"disabled\", \"disabled\");\n                    // enable the submit button\n                    $(\"#ddmZipCodeUpdateBtn\").removeClass(\"a-button-disabled\");\n                } else {\n                    $(\"#ddmZipCodeInput1\").removeAttr(\"disabled\");\n                    $(\"#ddmZipCodeInput2\").removeAttr(\"disabled\");\n\n                    // if we have valid pin code in pin code input box, keep the button enabled\n                    // else disable the button\n                    if(! isValidZipCode(retrieveZipCode())) {\n                        $(\"#ddmZipCodeUpdateBtn\").addClass(\"a-button-disabled\");\n                    }\n                }\n            });\n            $(document).delegate(\"#ddmCustomerAddress\", \"click\", function(event){\n                updateDeliveryMessage(1,defaultShippingCountryCode,defaultShippingZipCode,1);\n            });\n\n            isInit = true;\n        };\n        var retrieveZipCode = function() {\n            return $(\"#ddmZipCodeInput1\").val() + \"-\" + $(\"#ddmZipCodeInput2\").val();\n        }\n        var isValidZipCode = function(zip) {\n            return (zip && zip.match(/^[0-9]{3}.[0-9]{4}$/));\n        }\n        this.reloadDeliveryMessage = function() {\n            $(shippingMessageSectionId).hide();\n            updateDeliveryMessage(0,\"\",\"\",0);\n        };\n        this.destroy = function() {\n            $(document).undelegate(\"#ddmZipCodeInput1,#ddmZipCodeInput2\", \"focus input\");\n            $(document).undelegate(\"#ddmZipCodeUpdateBtn\", \"click\");\n            $(document).undelegate(\"#ddmCustomerAddress\", \"click\");\n            $(document).undelegate(\"#ddmCustomerAddressCheckbox\", \"change\");\n        };\n        var updateDeliveryMessage = function(updateSembu,countryCode,zipCode,useDefaultShippingAddress) {\n            countdownComp.stop();\n            \n            // clear content\n            $(\"#ddmZipCodeInput1\").val(\"\");\n            $(\"#ddmZipCodeInput2\").val(\"\");\n            if ($(\"#ddmCustomerAddressCheckbox\").attr(\"checked\")) {\n                $(\"#ddmCustomerAddressCheckbox\").parent(\"label\").click();\n            }\n            // disable button\n            $(\"#ddmZipCodeUpdateBtn\").addClass(\"a-button-disabled\");\n            \n            // hide all the popovers\n            P.when(popupType).execute(function(popup) {\n                popup.get(popupName) && popup.get(popupName).hide();\n            });\n\n            $.ajax({\n                type: \"POST\",\n                url: updateDeliveryMessageUrl,\n                contentType: 'application/x-www-form-urlencoded;charset=utf-8',\n                data: {\"deviceType\":deviceType,\"asin\":asin,\"merchantId\":merchantId,\n                       \"updateSembu\":updateSembu,\"countryCode\":countryCode,\"zipCode\":zipCode,\n                       \"useDefaultShippingAddress\":useDefaultShippingAddress},\n                dataType: \"json\",\n                success: function(data){\n                    var selectedAddressText = (data.useDefaultShippingAddress == 1) ? data.strDefaultShippingAddress : data.selectedAddressText;\n\n                    $(\"#ddmSelectedAddressText\").html(selectedAddressText);\n                    $(\"#ddmShippingMessage\").html(data.fastShippingMessage);\n                    if (data.template == \"deliverable_message\") {\n                        $(\"#ddmSelectionBoxLeft\").html(data.strDeliveryTo);\n                        $(\"#holidayAvailabilityMessage_feature_div\").html(data.hamString);\n                        $(\"#holidayDeliveryMessage_feature_div\").html(data.hdmString);\n                        countdownComp.start(data.fastSecondsLeftToOrder,0);\n                    } else {\n                        $(\"#ddmSelectionBoxLeft\").html(data.undeliverableMessage);\n                        $(\"#holidayAvailabilityMessage_feature_div\").html(\"\");\n                        $(\"#holidayDeliveryMessage_feature_div\").html(\"\");\n                    }\n                    $(shippingMessageSectionId).show();\n                },\n                error: function(){\n                    $(\"#dynamicDeliveryMessage\").replaceWith(strings.serverBusy);\n                    $(\"#holidayAvailabilityMessage_feature_div\").html(\"\");\n                    $(\"#holidayDeliveryMessage_feature_div\").html(\"\");\n                }\n            });\n        };\n    };\n});\n<\/script>\n\n            <script type=\"text/javascript\">\n/**\n * Component: used to create