  <div class="row row-well">
    <div class="col-xs-5">
      

<div class="input-group amount-control">
  <span class="input-group-btn">
    <a id="ctl00_Main_mengenControl_hlMinus" disabled="disabled" class="btn btn-default"><i class="icon-minus"></i></a>
  </span>
  <input name="ctl00$Main$mengenControl$mengenFeld" value="1" id="ctl00_Main_mengenControl_mengenFeld" disabled="disabled" class="form-control" type="number" />
  <span class="input-group-btn">
    <a id="ctl00_Main_mengenControl_hlPlus" disabled="disabled" class="btn btn-default"><i class="icon-plus"></i></a>
  </span>
</div>

<script type="text/javascript">

  var mengenc_anzahl = parseInt(1);
  var minAmount = parseInt(0);
  var maxAmount = parseInt(9999);
  var multiMenge = parseInt(1);
  var stockMenge = parseInt(0);
  var existsStaffel = parseInt(0);
  var listStaffelPreise = new Array();
  
  var popovertimeout;
  
  $(document).ready(function () {

      $('#priceGroup').popover({container: 'body', template: '<div class="popover popover-info"><div class="arrow"></div><h3 class="popover-title"></h3><div class="popover-content"></div></div>'});
      
    

    if (mengenc_anzahl == 1 && multiMenge > 1) {
      mengenc_anzahl = multiMenge;
    }

    if ((mengenc_anzahl == 1) ||
        (mengenc_anzahl == minAmount) ||
        ((mengenc_anzahl - multiMenge) <= minAmount)) {
      $("#ctl00_Main_mengenControl_hlMinus").addClass('disabled');
    }
    if ((mengenc_anzahl == maxAmount) ||
        ((mengenc_anzahl + multiMenge) > maxAmount)) {
      $("#ctl00_Main_mengenControl_hlPlus").addClass('disabled');
    }

      $("#ctl00_Main_mengenControl_mengenFeld").val(mengenc_anzahl);

      if (mengenc_anzahl >= 0 && 0 > 1) {
          $('#preisReduced').show();
      $('#preisNormal').hide();
      }
      else {
                $('#preisReduced').hide();
      $('#preisNormal').show();
      }

      setScalePrice();

    

    $("#ctl00_Main_mengenControl_mengenFeld").on("change", function (e, u) {

      mengenc_anzahl = parseInt(document.getElementById('ctl00_Main_mengenControl_mengenFeld').value);
      if (isNaN(mengenc_anzahl)) {
        mengenc_anzahl = 0;
      }
      $("#ctl00_Main_mengenControl_hlPlus").removeClass('disabled');
      $("#ctl00_Main_mengenControl_hlMinus").removeClass('disabled');

      if (mengenc_anzahl <= 0) {
        mengenc_anzahl = 1;
      }

      if (mengenc_anzahl < minAmount) {
        mengenc_anzahl = minAmount;
      }

      if (mengenc_anzahl >= maxAmount) {
        //alert('getMaxOrderError');
        mengenc_anzahl = maxAmount;
        $("#ctl00_Main_mengenControl_hlPlus").addClass('disabled');
          $("#ctl00_Main_mengenControl_hlMinus").removeClass('disabled');
        } else {
          $("#ctl00_Main_mengenControl_hlPlus").removeClass('disabled');
        }

      if (mengenc_anzahl > stockMenge) {
        //alert('getOutOfStockError');
        mengenc_anzahl = stockMenge;
        $("#ctl00_Main_mengenControl_hlPlus").addClass('disabled');
          $("#ctl00_Main_mengenControl_hlMinus").removeClass('disabled');
        }

      if ((multiMenge > 0) && (mengenc_anzahl % multiMenge != 0)) {
        while (mengenc_anzahl % multiMenge != 0) {
          mengenc_anzahl++;
        }
        $("#ctl00_Main_mengenControl_hlMinus").removeClass('disabled');
        }

      if (multiMenge > 1 && (mengenc_anzahl - multiMenge) < 1) {
        $("#ctl00_Main_mengenControl_hlMinus").addClass('disabled');
        }
      if (mengenc_anzahl + 1 > maxAmount || mengenc_anzahl + 1 > stockMenge) {
        $("#ctl00_Main_mengenControl_hlPlus").addClass('disabled');
        }

      if (mengenc_anzahl <= 1) {
        mengenc_anzahl = 1;
        $("#ctl00_Main_mengenControl_hlMinus").addClass('disabled');
        }

        document.getElementById('ctl00_Main_mengenControl_mengenFeld').value = mengenc_anzahl;

        if (mengenc_anzahl >= 0 && 0 > 1) {
            if ($('#preisNormal').is(":visible")) {
                $('#priceGroup').popover('show');
                clearTimeout(popovertimeout);
                popovertimeout = setTimeout(function(){ $('#priceGroup').popover('hide')}, 5000);
            }
            $('#preisReduced').show();
            $('#preisNormal').hide();
        }
        else {
            if ($('#preisReduced').is(":visible")) {
                $('#priceGroup').popover('show');
                clearTimeout(popovertimeout);
                popovertimeout = setTimeout(function(){ $('#priceGroup').popover('hide')}, 5000);
            }
            $('#preisReduced').hide();
            $('#preisNormal').show();
        }
        
        setScalePrice();
        
    });

    if ("ontouchstart" in document.documentElement) {
      $("#ctl00_Main_mengenControl_hlPlus").on("touchend", add);
      $("#ctl00_Main_mengenControl_hlMinus").on("touchend", subtr);
    } else {
      $("#ctl00_Main_mengenControl_hlPlus").on("click", add);
      $("#ctl00_Main_mengenControl_hlMinus").on("click", subtr);
    }

    //Allow only numbers
    //$("#ctl00_Main_mengenControl_mengenFeld").ForceNumericOnly();

  });

  function add() {

    // Maximale Menge entsprechend der Multimenge korrigieren.
    maxAmount = parseInt(maxAmount / multiMenge) * multiMenge;
    if (maxAmount < multiMenge) {
      maxAmount = multiMenge;
    }

    mengenc_anzahl++;
    $("#ctl00_Main_mengenControl_hlMinus").removeClass('disabled');

    if (mengenc_anzahl > maxAmount) {
      //alert('getMaxOrderError');
      mengenc_anzahl = maxAmount;
      $("#ctl00_Main_mengenControl_hlPlus").addClass('disabled');
    }

    if (mengenc_anzahl > stockMenge) {
      //alert('getOutOfStockError');
      $("#ctl00_Main_mengenControl_hlPlus").addClass('disabled');
      mengenc_anzahl = stockMenge;
    }

    if ((multiMenge > 0) && (mengenc_anzahl % multiMenge != 0)) {
      while (mengenc_anzahl % multiMenge != 0) {
        mengenc_anzahl++;
      }
    }

    if (mengenc_anzahl + 1 > maxAmount || mengenc_anzahl + 1 > stockMenge) {
      $("#ctl00_Main_mengenControl_hlPlus").addClass('disabled');
    }

      document.getElementById('ctl00_Main_mengenControl_mengenFeld').value = mengenc_anzahl;

      if (mengenc_anzahl >= 0 && 0 > 1) {
          if ($('#preisNormal').is(":visible")) {
              $('#priceGroup').popover('show');
              clearTimeout(popovertimeout);
              popovertimeout = setTimeout(function(){ $('#priceGroup').popover('hide')}, 5000);
          }
          $('#preisReduced').show();
          $('#preisNormal').hide();
      }
      else {
          if ($('#preisReduced').is(":visible")) {
              $('#priceGroup').popover('show');
              clearTimeout(popovertimeout);
              popovertimeout = setTimeout(function(){ $('#priceGroup').popover('hide')}, 5000);
          }
          $('#preisReduced').hide();
          $('#preisNormal').show();
      }
      setScalePrice();
  }

  function subtr() {
    mengenc_anzahl--;
    $("#ctl00_Main_mengenControl_hlPlus").removeClass('disabled');

    if (mengenc_anzahl < minAmount) {
      mengenc_anzahl = minAmount;
      $("#ctl00_Main_mengenControl_hlMinus").addClass('disabled');
    }
    else if ((multiMenge > 0) && (mengenc_anzahl % multiMenge != 0)) {
      while (mengenc_anzahl % multiMenge != 0) {
        mengenc_anzahl--;
      }
      if (multiMenge != 1 && mengenc_anzahl < multiMenge) {
        mengenc_anzahl = multiMenge;
        $("#ctl00_Main_mengenControl_hlMinus").addClass('disabled');
      }
  }

  if (mengenc_anzahl <= 1) {
    mengenc_anzahl = 1;
    $("#ctl00_Main_mengenControl_hlMinus").addClass('disabled');
  }

  if (multiMenge > 1 && (mengenc_anzahl - multiMenge) < 1) {
    $("#ctl00_Main_mengenControl_hlMinus").addClass('disabled');
  }
    $("#ctl00_Main_mengenControl_mengenFeld").val(mengenc_anzahl);
      if (mengenc_anzahl >= 0 && 0 > 1) {
          $('#preisReduced').show();
          if ($('#preisNormal').is(":visible")) {
              $('#priceGroup').popover('show');
              clearTimeout(popovertimeout);
              popovertimeout = setTimeout(function(){ $('#priceGroup').popover('hide')}, 5000);
          }
          $('#preisReduced').show();
          $('#preisNormal').hide();
    }
    else {
         
          if ($('#preisReduced').is(":visible")) {
              $('#priceGroup').popover('show');
              clearTimeout(popovertimeout);
              popovertimeout = setTimeout(function(){ $('#priceGroup').popover('hide')}, 5000);
          }
          $('#preisReduced').hide();
          $('#preisNormal').show();
    }
      setScalePrice();
  }

    function setScalePrice()
    {
        if (existsStaffel)
        {
            var staffelIndex = 0;
            for (var i = listStaffelPreise.length - 1; i >= 0; i--)
            {
                if (mengenc_anzahl >= listStaffelPreise[i])
                {
                    staffelIndex = i;
                    break;
                }
            }

            $('[data-staffelpreisindex]').hide();
            if (staffelIndex > 0) {
                $('[data-staffelpreisindex="' + staffelIndex + '"]').show();
                $('#preisNormal').hide();
            }
            else
            {
                $('#preisNormal').show();
            }

            $('[data-staffelbuttonindex]').removeClass('active');
            $('[data-staffelbuttonindex="' + staffelIndex + '"]').addClass('active');

            $('[data-staffelbasepreisindex]').hide();
            $('[data-staffelbasepreisindex="' + staffelIndex + '"]').show();
        }
  }

</script>

    </div>
    <div class="col-xs-7 col-sm-offset-1 col-sm-6 col-lg-offset-4 col-lg-3">
      
      <a id="ctl00_Main_btnAddToCart" disabled="disabled" class="btn btn-block btn-buy" onclick="ROLMOBILE.shopexd.addToCart()" data-toggle="modal"><i class="icon-shopping-cart"></i>&nbsp;In den Warenkorb</a>
    </div>

    

