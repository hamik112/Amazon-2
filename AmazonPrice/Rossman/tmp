/*
    Dirk: Anzeige des Side-Teasers auf unterschiedlichen Seiten mit Ablaufdatum ... A-Datum, Grafik und Link anpassen
*/
function sideTeaser() {
    var aktuell = new Date();
       aktuell = aktuell.getTime();
    var ende = new Date(2016,7,31);
       ende = ende.getTime();
    if (ende > aktuell && screen.width>1199) {
        show = '<div style="display:block"><div id="holder-side-teaser"><div style="color:#fff" id="side-teaser"><a href="http://www.rossmannversand.de/kategorie/angebote-und-aktionen/garten/216911.aspx" title="zur Themenseite"><img src="http://www.rossmannversand.de/Portals/_eEvolutionCMS/images/default/shop/sideteaser/Stoerer_Startseite_Garten.png"></a></div></div></div>';
    } else {
        show = '<div style="display:none"></div>';
    }
    document.getElementById("anzeigenbis").innerHTML = show;
}
