                'colorImages': { 'initial': [{"hiRes":"http://ecx.images-amazon.com/images/I/71N6tuY0mHL._SL1225_.jpg","thumb":"http://ecx.images-amazon.com/images/I/51Lfv-i-z6L._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/51Lfv-i-z6L.jpg","main":{"http://ecx.images-amazon.com/images/I/71N6tuY0mHL._SY445_.jpg":[445,298],"http://ecx.images-amazon.com/images/I/71N6tuY0mHL._SY500_.jpg":[500,335],"http://ecx.images-amazon.com/images/I/71N6tuY0mHL._SY550_.jpg":[550,368],"http://ecx.images-amazon.com/images/I/71N6tuY0mHL._SY606_.jpg":[606,406],"http://ecx.images-amazon.com/images/I/71N6tuY0mHL._SY679_.jpg":[679,455]},"variant":"MAIN"},{"hiRes":"http://ecx.images-amazon.com/images/I/71r49Ps053L._SL1500_.jpg","thumb":"http://ecx.images-amazon.com/images/I/51chE6cO8LL._SX38_SY50_CR,0,0,38,50_.jpg","large":"http://ecx.images-amazon.com/images/I/51chE6cO8LL.jpg","main":{"http://ecx.images-amazon.com/images/I/71r49Ps053L._SY445_.jpg":[445,240],"http://ecx.images-amazon.com/images/I/71r49Ps053L._SY500_.jpg":[500,270],"http://ecx.images-amazon.com/images/I/71r49Ps053L._SY550_.jpg":[550,297],"http://ecx.images-amazon.com/images/I/71r49Ps053L._SY606_.jpg":[606,327],"http://ecx.images-amazon.com/images/I/71r49Ps053L._SY679_.jpg":[679,367]},"variant":"PT01"}]},
