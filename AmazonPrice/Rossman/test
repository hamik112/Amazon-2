<script type="text/javascript">
  $(document).ready(function () {
    if ($('.checkbox').length > 0) {
      $('.checkbox').dgStyle();
      $('.filters').find('label').removeAttr('for');
      $('.singleFilter').not('.disabled').click(function (e) {
        $(this).find('.checkbox').dgHandle();
        e.stopPropagation();
      });
      $('.clfilterboxen input:checkbox').click(function (e) {
        loadimg = $('.loadIcon');
        $(this).parents('.singleFilter').addClass('loading');
        $(this).parents('.singleFilter').find('.prodCount').after($('.loadwrapper'));

        //Wenn eine Checkbox abgehakt wird und nurnoch angehakte Filter mit 0 Produkten übrig bleiben würden, sollen diese auch ein Ladesymbol bekommen, da sie ausgeblendet werden in der nächsten Seite
        //Ist noch mehr als ein Filter mit Produkten vorhanden bekommen die 0 Filter kein Ladesymbol
        if (this.attributes['checked'] != null) {
          if ($(this).parents('.filtermidcheck').find('.singleFilter.geloaded').not('.disabled').length < 2) {
            $(this).parents('.filtermidcheck').find('.disabled').addClass('loading').find('.prodCount').after($('.loadwrapper').clone());
          }
        }
        $('#filterWrapper').addClass('modal_backdrop');

        var angle = 0;
        setInterval(function () {
          angle += 3; $('.loadIcon').rotate(angle);
        }, 50);

        $(this).parents('.singleFilter').unbind('click').unbind('mousedown').unbind('mouseup');
        ChangeCheckBox($(this).attr('id'));
        $(this).unbind('click');
        e.stopPropagation();
      });
      $('.selectionOn').click(function () {
        $(this).removeClass('selectionOn').addClass('selectionOffload');
        $(this).find('.fa-times').hide();
        $(this).find('#loadWrapper').append($('#loadIconSmall'));

        $('#filterWrapper').addClass('modal_backdrop');

        var angle = 0;
        setInterval(function () {
          angle += 3; $('#loadIconSmall').rotate(angle);
        }, 50);

        ChangeCheckBox('dummy');

      });
      $('.selectionOff').click(function () {
        return false;
      });
      $('.allFilterOff').click(function () {
        return false;
      });
      $('.allFilterOn').click(function () {
        $(this).removeClass('allFilterOn').addClass('allFilterOffload');
        $('#filterWrapper').addClass('modal_backdrop');
        $(this).find('.fa-times').hide();
        $(this).find('#loadKreisAllOff').show();        
        var angle = 0;
        setInterval(function () {
          angle += 3; $('#loadIconOff').rotate(angle);
        }, 50);
        ChangeCheckBox('dummy');

      });
      $('a#TitleListFilter').click(function () {
        HideShowFilter($(this));
      });
      $('*[data-flag=ButtonShowPriceRange]').click(function (e) {
        if ($(this).hasClass('activ')) {
          $(this).find('#loadWrapper').append($('#loadIconSmall'))
          var angle = 0;
          setInterval(function () {
            angle += 3; $('#loadIconSmall').rotate(angle);
          }, 50);
          ChangeCheckBox($(this).attr('id'));
        }
        else {
          return false;
        }
      });
      //FilterMehrButton
      $('.showMoreItems').click(showMoreItems);

      //Markenoverlay
      $('.showMoreMarken').click(showMoreMarken);
    }
  });

</script>
