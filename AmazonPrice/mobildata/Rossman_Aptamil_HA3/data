        <script>

            function ecMark(src, markername) {
                var emospro = {};
                emospro.siteid = 'm.rossmannversand.de';
                emospro.marker = "Mobile/" + whereAmI + "/" + src + "/" + markername;
                window.emosPropertiesEvent(emospro);
                //console.log(emospro.marker);
            }

            jQuery.fn.ForceNumericOnly =
              function () {
                  return this.each(function () {
                      $(this).keydown(function (e) {
                          var key = e.charCode || e.keyCode || 0;
                          return (
                              key == 8 ||
                              key == 9 ||
                              key == 46 ||
                              (key >= 37 && key <= 40) ||
                              (key >= 48 && key <= 57) ||
                              (key >= 96 && key <= 105)
                          );
                      });
                  });
              };

            $(document).ready(function () {

                // X Icons nach partial postback initialisieren
                Sys.WebForms.PageRequestManager.getInstance().add_endRequest(ROLMOBILE.master.showHideTextboxXIcons);

                // Wk Anzahl aktualisieren
                ROLMOBILE.master.updateHeaderCartCount();

                // X Icons in Textboxen anzeigen/ausblenden
                ROLMOBILE.master.showHideTextboxXIcons();

                //FACT-Finder Suggest initialisieren
                var suggest = new ROLMOBILE.factfinder.suggest.FFSuggest();
                suggest.initDefault();

                // OC-Menu initialisieren
                ROLMOBILE.ocMenu.init();

                // Search Button
                $('#ctl00_searchPanel').on('shown.bs.collapse', function () {
                    ROLMOBILE.ocMenu.hide();
                    $('#searchfield').focus();
                });

                var searchBtn = $('#ctl00_hSearch');
                if (searchBtn.length != 0) {
                    $(searchBtn).on("click", function () {
                        $(searchBtn).toggleClass('active');
                    });
                }

                ROLMOBILE.ocMenu.menulink.fastClick(function () {
                    if ($('#ctl00_searchPanel').hasClass('in')) {
                        $('#ctl00_searchPanel').collapse('hide');
                         $('#ctl00_hSearch').removeClass('active');
                         $('.navbar-fixed-top').removeClass('navbar-absolute-top');
                     }
                 });

                if ($(".btn[data-scroll-to='top']").length != 0) {
                    $(".btn[data-scroll-to='top']").fastClick(function () {
                        //nach oben Scrollen
                        $("html, body").animate({ scrollTop: 0 }, "slow");
                        return false;
                    });
                }
            });
        </script>
        
    

