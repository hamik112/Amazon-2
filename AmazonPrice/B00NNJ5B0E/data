            <div class="a-popover-preload" id="a-popover-ddmPopoverBlock">
                <div class="a-section">
                    
                    <form method="post" action="javascript:;" class="a-spacing-none">
                    <table class="a-normal">
                        
                        <tr>
                            <td class="a-text-right a-align-center">
                                <span class="a-text-bold">郵便番号:</span>
                            </td>
                            <td class="a-text-left a-align-center">
                                <input type="text" maxlength="3" id="ddmZipCodeInput1" class="a-input-text a-width-small">
                                <span>-</span>
                                <input type="text" maxlength="4" id="ddmZipCodeInput2" class="a-input-text a-width-small">
                                <span id="ddmZipCodeUpdateBtn" class="a-button a-button-primary a-button-small"><span class="a-button-inner"><input class="a-button-input" type="submit" aria-labelledby="ddmZipCodeUpdateBtn-announce"><span id="ddmZipCodeUpdateBtn-announce" class="a-button-text" aria-hidden="true">Go</span></span></span>
                            </td>
