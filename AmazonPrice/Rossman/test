  <script language="javascript" type="text/javascript">

    var _updateProgressDiv;
    var _backgroundDiv;
    var _gridView;

    function pageLoad(sender, args) {
      //  register for our events
      Sys.WebForms.PageRequestManager.getInstance().add_beginRequest(beginRequest);
      Sys.WebForms.PageRequestManager.getInstance().add_endRequest(endRequest);

      _gridView = $get('wrapper');

      //  create the div that we will position over the gridview
      //  during postbacks
      if (document.getElementById("divDisabler") == null) {
        _backgroundDiv = document.createElement('div');
        _backgroundDiv.style.display = 'none';
        _backgroundDiv.style.zIndex = 32000;
        //_backgroundDiv.style.backgroundColor = "gray";
        _backgroundDiv.id = "divDisabler";
        _backgroundDiv.className = 'modal_background';
        _backgroundDiv.style.position = 'absolute';

        //  add the element to the DOM
        _gridView.parentNode.appendChild(_backgroundDiv);
      }

      Sys.Browser.WebKit = {};
      if (navigator.userAgent.indexOf('WebKit/') > -1) {
        Sys.Browser.agent = Sys.Browser.WebKit;
        Sys.Browser.version = parseFloat(navigator.userAgent.match(/WebKit\/(\d+(\.\d+)?)/)[1]);
        Sys.Browser.name = 'WebKit';
      }
    }
    //updatepanel aktualisert - klickblocker anschalten
    function beginRequest(sender, args) {

      _backgroundDiv.style.display = '';

      var gridViewBounds = Sys.UI.DomElement.getBounds(_gridView);

      $(_backgroundDiv).offset($(_gridView).offset());

      _backgroundDiv.style.width = gridViewBounds.width + 'px';
      _backgroundDiv.style.height = gridViewBounds.height + 'px';

      _gridView.enabled = false;
    }

    //updatepanel ist fertig
    function endRequest(sender, args) {
      _backgroundDiv.style.display = 'none';
      cartProductInfoBehaviour();
      cartDeliveryInfoBehaviour();
      var length = quantityScripts.length, element = null;

      for (var i = 0; i < length; i++) {
        (quantityScripts[i])();
      }

      //Höhe der Warenkorbpositionen angleichen/updaten
      //$("[data-mh='cart-pos-col']").matchHeight();
    }

    function UpdateCart() {
      __doPostBack('ctl00_Main_updatepanel1', '');
    }

    $(document).ready(function () {
      cartProductInfoBehaviour();
      cartDeliveryInfoBehaviour();

    });

  </script>

  

