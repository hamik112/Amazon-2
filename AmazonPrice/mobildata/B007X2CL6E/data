<script>
    (function () {
        var searchBoxID = 'searchKeyword';
        function fixForIPhone5 () {
            var elem = document.getElementById(searchBoxID);
            if (Math.abs(window.orientation) == 90) {
                elem.style.width = '100%';
            } else {
                elem.style.width = 'auto';
            }
        }
        window.addEventListener('orientationchange', fixForIPhone5, false);
    })();
