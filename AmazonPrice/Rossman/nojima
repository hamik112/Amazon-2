<script type="text/javascript">
jQuery(document).ready(function() {

  
  var ua_smartphone = /Android|iPhone/, 
  order_url = /\/app\/order/,
  customer_edit2_url = /\/app\/customer\/customer_edit2/,
  customer_confirm_url = /\/app\/customer\/customer_confirm/,
  ranking_url = /\/app\/common\/sales_ranking_category/, 
  review_ranking_url = /\/app\/common\/review_all_ranking/,
  campaign_url = /\/ec\/shop\/[^\/]+\/campaign/, 
  all_campaign_url = /\/app\/catalog\/campaign_list/, 
  contents_guide_madoguti_url = /\/ec\/siteinfo\/guide/,
  contents_encho_hosho_url = /\/ec\/encho-hosho/,
  contents_oogata_url = /\/ec\/oogata/,
  contents_sitemap_url = /\/app\/info\/sitemap/,
  contents_faq_url = /\/ec\/faq/, 
  contents_enj_url = /\/ec\/enj*/, 
  contents_siteinfo_url = /\/ec\/siteinfo/, 
  contents_otoiawase_url = /\/ec\/otoiawase*/;
  var frontUrl = location.host + '/';
  var spUrl =  location.host + '/sp/';
  
  var expiresDay = 7;
  var cookiePath = '/';
  var url = document.URL;
  if(!ranking_url.test(url)
		  && !review_ranking_url.test(url)
		  && !campaign_url.test(url)
		  && !all_campaign_url.test(url)
		  && !contents_guide_madoguti_url.test(url)
		  && !contents_encho_hosho_url.test(url)
		  && !contents_oogata_url.test(url)
		  && !contents_sitemap_url.test(url)
		  ){
      if (ua_smartphone.test(navigator.userAgent)) {

          if(!$.cookie('MODE') || $.cookie('MODE') == "SP"){
        	  
              if(contents_faq_url.test(url) || contents_enj_url.test(url) || contents_siteinfo_url.test(url) || contents_otoiawase_url.test(url)){
            	
                frontUrl = '/ec/';
                spUrl = '/sp/ec/SM_';
                
              }
              
              $.cookie('MODE', 'SP', { path: cookiePath,expires: expiresDay });
              location.href = document.URL.replace(frontUrl, spUrl);
          }else{
              if(!order_url.test(url) && !customer_edit2_url.test(url) && !customer_confirm_url.test(url)){
            	  
                  jQuery('#moveSpButton').css("display","block");
              }
              $.cookie('MODE', 'PC', { path: cookiePath,expires: expiresDay });
          }
      }
  }

});
</script>
