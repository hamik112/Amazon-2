    <script>
        var ROLMOBILE = ROLMOBILE || {};
        var whereAmI;

        ROLMOBILE.getQueryStringParam = function (name) {
            name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
            var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
                results = regex.exec(location.search);
            return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
        }

        ROLMOBILE.master = {
            aspnetVars: new Array(),
            eEvoApplicationRoot: '',
            cartItems: parseInt('0'),
            logoutModalboxId: '#ctl00_ModalBox_modal',
            catId: 0,

            regSrvVar: function (key, value) {
                this.aspnetVars[key] = value;
            },

            updateHeaderCartCount: function (cartTotalQuantity) {

                if (cartTotalQuantity != null) {
                    this.cartItems = cartTotalQuantity;
                }
                $('#iCartCount').html(this.cartItems);

                if (this.cartItems > 0) {
                    $('#ctl00_btnCart').removeClass("btn-default").addClass("btn-buy");
                  } else {
                      $('#ctl00_btnCart').removeClass("btn-buy").addClass("btn-default");
                  }

              },

            handleSearchClick: function () {
                if ($('#searchfield').val() != "") {
                    window.location.href = ROLMOBILE.master.eEvoApplicationRoot + "/DesktopModules/WebShop/mobile_shopresultsearch.aspx?k=" + $('#searchfield').val();
                }
            },

            deleteInputText: function (e) {
                e.previousElementSibling.value = "";
                $(e.previousElementSibling).trigger("input");
                $(e).addClass("hidden");
                ROLMOBILE.factfinder.suggest.hideLayerOutsideCall();
            },

            // Called initial
            showHideTextboxXIcons: function () {

                $(':input[data-clearable=true]').on('input', function (event) {
                    if ($(event.currentTarget).val().length > 0) {
                        $(event.currentTarget.nextElementSibling).removeClass("hidden");
                        $(event.currentTarget).attr("data-isEmpty", false);
                    } else {
                        $(event.currentTarget.nextElementSibling).addClass("hidden");
                        $(event.currentTarget).attr("data-isEmpty", true);
                    }
                });

                $.each($('[data-clearable=true]'), function (key, value) {
                    if (!$(value).prop('disabled')) {
                        $(value).trigger('input');
                    }
                });
            },



            goBack: function () {

                // Check whether the referrer is our host or not
                var ref = document.referrer;

                if (ref.search(window.location.host) != -1) {
                    history.back();
                } else {
                    window.location.href = ROLMOBILE.master.eEvoApplicationRoot;
                }

            },

            closeLoginAdvice: function (e) {
                $.ajax({
                    type: "POST",
                    url: this.eEvoApplicationRoot + "/WebService/Mobile_WebService.asmx/CloseLoginAdvice",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function () {
                        $(e).parent().remove();
                    },
                    error: function (x, e) {
                        console.log("The call to the server side failed. " + x.responseText);
                    }
                });
            },

            logout: function () {

                $('#ctl00_btnLogout').click();

              },
            rememberClickedProduct: function (clickedElement, id) {
                //Unterscheidung Suche/Kategorie
                if (ROLMOBILE.shopdisplayProducts != null) {
                    ROLMOBILE.master.catId = ROLMOBILE.shopdisplayProducts.catId;
                }
                else {
                    if (ROLMOBILE.shopresultsearch != null) {
                        ROLMOBILE.master.catId = ROLMOBILE.shopresultsearch.ff_kw + ROLMOBILE.shopresultsearch.ff_marke;
                    }
                }

                var child = $(clickedElement).closest(".row");
                $.jStorage.set("productListPosition", child.index());
                $.jStorage.set("productListPositionCat", ROLMOBILE.master.catId);
            },

            setAmount: function (mengeClientID, newAmount, onProductDetailsPage) {
                if (onProductDetailsPage) {
                }
                $("[id=" + mengeClientID + "]").val(newAmount);
                $("[id=" + mengeClientID + "]").trigger("change");

            }

        };


    </script>
