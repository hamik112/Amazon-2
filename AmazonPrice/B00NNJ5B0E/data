                    cheapSecondsLeftToOrder: 0,
                    addressPopover : {
                        name: "ddmPopoverBlock",
                        type: "a-popover",
                    },
                    showAndInCountDown: false,
                    defaultShippingCountryCode: "",
                    defaultShippingZipCode: "",
                    strings: {
                        hour: "時間",
                        hours: "時間",
                        minute: "分",
                        minutes: "分",
                        second: "秒",
                        seconds: "秒",
                        refreshTimeout: "新しいお届け予定日を見るにはこちらをクリック",
                        timeSeperator: " ",
                        and: "と",
                        serverBusy: "サーバーに接続できませんでした。ネットワーク接続をご確認のうえ、再試行してください。"
                    }
                };
                controllerComp.run(ddmContext);
            });
            </script>
            
